    s.set_constant('SERCA.Km_SERCA', x[4])
    s.set_constant('SERCA.V_max_SERCA_PLN', x[5])
    s.set_constant('SERCA.Km_SERCA_PLN', x[6])
    s.set_constant('SERCA.V_max_SERCA_SLN', x[7])
